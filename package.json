{
  "name": "vite-react-tailwind-eslint-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "biome format ./src",
    "format:prettier": "prettier . --check --config .prettierrc.mjs --ignore-path .prettierignore",
    "format:biome": "biome format ./src",
    "format:biome:fix": "biome format --verbose ./src --write",
    "test": "jest",
    "check": "npm run lint && npm run format:biome && npm run test",
    "typecheck": "tsc -p tsconfig.json",
    "dev": "vite --host",
    "build": "npm run build:sw:dev && vite build",
    "preview": "vite preview",
    "build:sw": "export NODE_ENV=production && node ./build.config.js",
    "build:sw:dev": "export NODE_ENV=development && node ./build.config.js"
  },
  "dependencies": {
    "@ark-ui/react": "^5.31.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@reduxjs/toolkit": "^2.11.2",
    "@talkohavy/filters": "^3.0.0",
    "@talkohavy/lodash": "^1.4.1",
    "@talkohavy/table": "^1.0.12",
    "@tanstack/react-virtual": "^3.13.18",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "create-query-language": "^1.2.0",
    "highlight.js": "^11.11.1",
    "prismjs": "^1.30.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-redux": "^9.2.0",
    "react-router": "^7.13.0",
    "react-router-dom": "^7.13.0",
    "react-simple-code-editor": "^0.14.1",
    "react-tooltip": "^5.30.0",
    "socket.io-client": "^4.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.15",
    "@eslint/js": "^10.0.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/prismjs": "^1.26.6",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsonc-parser": "^3.3.1",
    "oxlint": "^1.47.0",
    "oxlint-tsgolint": "^0.12.1",
    "prettier": "^3.8.1",
    "sass-embedded": "^1.97.3",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "npm:rolldown-vite@latest",
    "vite-plugin-svgr": "^4.5.0"
  },
  "packageManager": "pnpm@10.30.0"
}
