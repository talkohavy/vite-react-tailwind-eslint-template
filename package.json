{
  "name": "vite-react-tailwind-eslint-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean": "rm -rf node_modules",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "biome format ./src",
    "format-fix": "biome format --verbose ./src --write",
    "format-prettier": "prettier . --check --config .prettierrc.mjs --ignore-path .prettierignore",
    "test": "jest",
    "typecheck": "tsc -p tsconfig.json",
    "dev": "vite --host",
    "build": "npm run build:sw:dev && vite build",
    "preview": "vite preview",
    "build:sw": "export NODE_ENV=production && node ./build.config.js",
    "build:sw:dev": "export NODE_ENV=development && node ./build.config.js"
  },
  "dependencies": {
    "@ark-ui/react": "^5.23.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@reduxjs/toolkit": "^2.8.2",
    "@talkohavy/filters": "^2.0.0",
    "@talkohavy/lodash": "^1.4.1",
    "axios": "^1.10.0",
    "clsx": "^2.1.1",
    "create-query-language": "^1.0.9",
    "highlight.js": "^11.11.1",
    "prismjs": "^1.30.0",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.7.0",
    "react-router-dom": "^7.7.0",
    "react-simple-code-editor": "^0.14.1",
    "react-tooltip": "^5.29.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@eslint/js": "^9.31.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "esbuild": "^0.25.6",
    "eslint": "^9.31.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "json-easy-strip": "^1.0.6",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.89.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.0.5",
    "vite-plugin-svgr": "^4.3.0"
  }
}
